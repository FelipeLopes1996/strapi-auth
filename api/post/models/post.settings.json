{
  "kind": "collectionType",
  "collectionName": "posts",
  "info": {
    "name": "post",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "attributes": {
    "title": {
      "type": "string",
      "required": true,
      "minLength": 5,
      "unique": false,
      "maxLength": 200
    },
    "content": {
      "type": "richtext",
      "required": true,
      "minLength": 10
    },
    "user": {
      "plugin": "users-permissions",
      "model": "user",
      "via": "posts"
    }
  }
}
